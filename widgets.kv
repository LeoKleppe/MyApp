<Mainwindow>:
    karbotall:karbotall
    insulinsvar:insulinsvar

    FloatLayout:
        size: root.width, root.height
        Button:
            text: 'Open popup'
            on_release: root.btn()
            size_hint: (0.3, 0.3)

        Button:
            text: '|||'
            on_release: root.show_menu()
            size_hint: (0.10, 0.07)
            pos_hint: {'x': 0, 'top': 1}

        BoxLayout:
            orientation: 'vertical'
            size_hint: (0.2, 0.2)
            pos_hint: {'top': 1, 'x': 0.3}

            Label:
                text: 'Inusulin Kalkulator'

            TextInput:
                id: karbotall
                multiline: False

            Button:
                on_press: root.kalkulate()
                text: 'Kalkulate'
                size_hint: (1,1.3)

            Label:
                id: insulinsvar
                text: ''

<Mainmenu>:
    Button:
        text: 'Min matt'
        size_hint: (1, 0.10)
        pos_hint: {'x': 0, 'top': 0.99}
        on_release: root.Min_matt()

<MatretterPopup>:
    Label:
        text: 'Matprat'
        size_hint: (0.3, 0.3)
        pos_hint: {'x': 0.2, 'top':1} 


<loginwindowstart>:
    size: root.width, root.height
    Label:
        text: 'Les meg!'
        size_hint: (0.3, 0.3)
        font_size: root.width / 15
        pos_hint: {'x': 0.35, 'top': 1.1}

    Label:
        text: ('Denne appen er for folk som har diabetes.\n' + 'I appen er det en Insulin kalkulator,\nmatvare lagring, sånn at du kan lagre fakta\nom middager, matretter og annet ting\n' + 'Appen bruker et 24 timers klokke system så når du\nlogger inn på siste side må du skrive i den formaten\n' + '\nLitt fakta om meg:\n' + 'Jeg begynnte å lage denne appen var jeg 14 år gammel,\n og jeg begynte på appen det året jeg fant ut at jeg hadde diabetes type 1.\n' + 'Håper du får bruk for appen!')
        size_hint: (0.3, 0.3)
        font_size: root.width / 35
        pos_hint: {'x': 0.35, 'top': 0.7}

    Button:
        text: 'Neste'
        on_release: root.neste() 
        size_hint: (None, None)
        size: root.width / 6, root.height / 10
        font_size: root.width / 50 + root.height / 50 % 2
        pos_hint: {'x': 0.4, 'top': 0.13}

    Button:
        text: 'Avbryt'
        on_release: root.close()
        size_hint: (None, None)
        size: root.width / 8, root.height / 10
        font_size: root.width / 50 + root.height / 50 % 2
        pos_hint: {'x': 0, 'top': 0.13}

<loginwindow>:
    deltallbl : antalldeltal
    name:name
    Label:
        text: 'Name'
        size_hint: (None, None)
        size: root.width / 12, root.height / 20
        font_size: (root.width / 50 + root.height / 50)
        pos_hint: {'x': 0.45, 'top': 0.95}

    TextInput:
        id: name
        multiline: False
        size_hint: (None, None)
        size: root.width / 6, root.height / 18
        pos_hint: {'x': 0.42, 'top':0.75}

    Button:
        text: 'Neste'
        pos_hint: {'x': 0.4, 'top': 0.13 }
        size_hint: (None, None)
        size: root.width / 6, root.height / 10
        font_size: root.width / 50 + root.height / 50 % 2
        on_release: root.showlogin2()

    Label:
        id: antalldeltal
        size_hint: (0.3, 0.3)
        font_size: 18
        text: ''
        pos_hint: {'x': 0.35, 'top': 0.425}

    Label:
        font_size:  (root.width / 50 + root.height / 50)
        text: 'Antall Delingstall på en dag'
        pos_hint: {'x': 0.35 , 'top': 0.73}
        size_hint: (0.3, 0.3)

    Slider:
        min: 1
        max: 6
        value: 1
        step: 1
        orientation: 'horizontal'
        size_hint: (None,None)
        size: root.width / 1.5, root.height % 10
        pos_hint: {'x': 0.15, 'top': 0.4}
        on_value: root.sliderdeltall(*args)

    Button:
        text: 'Avbryt'
        on_release: root.close()
        size_hint: (None, None)
        size: root.width / 8, root.height / 10
        font_size: root.width / 50 + root.height / 50 % 2
        pos_hint: {'x': 0, 'top': 0.13}

